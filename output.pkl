import "pkl:xml" as xml
import "android.pkl"
import "apple.pkl"
import "string.pkl"

localizable: string.Localizable
renderers: List<string.StringRenderer> = List(
    new android.AndroidStringRenderer {},
    new apple.AppleStringRenderer {}
)

hidden output {
    files {
        for (renderer in renderers) {
            ...renderer.render(localizable)
        }
    }
}