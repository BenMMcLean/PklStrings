amends "schema.pkl"
import "android.pkl"
import "apple.pkl"

localizable = new Localizable {
    languages = new Listing {}
}
renderers = new Listing {
    new android.AndroidStringRenderer {}
    new apple.AppleStringRenderer {}
}

output {
    files {
        for (renderer in renderers) {
            ...renderer.render(localizable)
        }
    }
}